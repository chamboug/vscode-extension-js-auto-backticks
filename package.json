{
	"name": "js-auto-backticks",
	"displayName": "Javascript Auto Backticks",
	"description": "Automatically converts single or double quotes to backticks when needed.",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#CE9178",
		"theme": "dark"
	},
	"publisher": "chamboug",
	"version": "1.0.0",
	"repository": "https://github.com/chamboug/vscode-extension-js-auto-backticks",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact",
		"onLanguage:vue"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"postversion": "git push --follow-tags",
		"lint": "eslint src/**/*.js",
		"lintFix": "eslint src/**/*.js --fix"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"babel-eslint": "^10.1.0",
		"eslint": "6.8.0"
	},
	"dependencies": {
		"@phenomnomnominal/tsquery": "5.0.0",
		"typescript": "4.6.4"
	}
}
